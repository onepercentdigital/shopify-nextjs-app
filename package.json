{
  "name": "shopify-nextjs-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "bun run --bun next build",
    "check": "biome check .",
    "check:all": "bun run type && bun run check",
    "dev": "bun run --bun next dev",
    "format": "biome format --write .",
    "lint": "biome lint .",
    "lint-fix": "biome check --write .",
    "lint-fix-unsafe": "biome check --write --unsafe .",
    "start": "bun run --bun next start",
    "type": "tsc --noEmit"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.9",
    "@heroicons/react": "^2.2.0",
    "clsx": "^2.1.1",
    "geist": "^1.5.1",
    "next": "15.6.0-canary.57",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "sonner": "^2.0.7"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/postcss": "^4.1.16",
    "@tailwindcss/typography": "^0.5.19",
    "@types/node": "^22.18.13",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.16",
    "typescript": "^5.9.3"
  },
  "trustedDependencies": [
    "@tailwindcss/oxide",
    "sharp",
    "unrs-resolver"
  ]
}
